use { Span } from "./span.brim"

fn parse(input: string) -> string !MyError {
     if input.len() <= 0 {
         return MyError::ParsingError {
             message: "Input is empty",
             span: Span::new(0, 0)
         }
     }

     return input
}

pub fn main() {
    let value = parse(input) catch |err: MyError| {
        println(err)
    }

    println("Value {}", [value])
}