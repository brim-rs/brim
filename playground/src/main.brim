pub enum Result<T, E> {
    Ok(T),
    Err(E),
};

pub fn div<T: i32, E: string>(a: i32, b: i32) -> T!E {
    if b == 0 {
        return Result<T, E>::Err("Division by zero");
    }

    return Result<T, E>::Ok(a / b);
};

pub fn main() {
    let x = 1;
    let y = -x;
    let z = 12;

    let res = div(z, y);
};