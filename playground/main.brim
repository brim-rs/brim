error MyError {
    ParsingError {
        message: string,
        span: Span
    }
}

fn parse(input: string) -> string !MyError {
     if input.len() <= 0 {
         return MyError::ParsingError {
             message: "Input is empty",
             span: Span::new(0, 0)
         }
     }

     return input
}

