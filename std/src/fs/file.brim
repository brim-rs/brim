use windows from "../os/windows.brim"
use { IoError } from "../io/error.brim";

pub type Handle = match @os() {
    "windows" => windows::HANDLE,
    "linux" => i32,
    "macos" => i32,
    else => i32
};

pub struct File {
    handle: Handle,

    fn write(self: &File, data: &[u8]) usize!IoError {
        return @ok(@cast(0, usize));
    }
}
